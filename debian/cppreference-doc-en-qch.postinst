#!/bin/bash

set -e

case "$1" in
    configure)
        assistant -register /usr/share/cppreference/doc/qch/cppreference-doc-en-cpp.qch
    ;;
    abort-upgrade|abort-remove|abort-deconfigure)
        echo "$1"
    ;;
    *)
        echo "postinst called with unknown argument \`\$1'" >&2
        exit 0
    ;;

#DEBHELPER#

esac
