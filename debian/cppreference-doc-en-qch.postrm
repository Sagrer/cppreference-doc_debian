#!/bin/bash

set -e

case "$1" in
    remove)
        assistant -unregister /usr/share/cppreference/doc/qch/cppreference-doc-en-cpp.qch
    ;;
    purge|upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
        echo "$1"
    ;;
    *)
        echo "postinst called with unknown argument \`\$1'" >&2
        exit 0
    ;;

#DEBHELPER#

esac
